@{
    Style.Include("style.css");
    Style.Include("#").SetAttribute("media", "screen").SetAttribute("class", "skin");
    Style.Include("ie/ie.css").SetAttribute("media", "screen").UseCondition("lte IE 8").AtHead();
    Script.Include("modernizr.js").AtHead();
    Script.Include("http://getbootstrap.com/docs-assets/js/html5shiv.js").UseCondition("lte IE 9").AtHead();
    Script.Include("responsive/respond.js").UseCondition("lte IE 9").AtHead();
    Script.Include("http://www.google.com/jsapi").AtHead();
}
@using (Script.Head()) {
    <!-- Favicons -->
    <link rel="shortcut icon" href="~/themes/orchardharvest.theme2017/images/icons/favicon.ico">
    <link rel="apple-touch-icon" href="~/themes/orchardharvest.theme2017/images/icons/apple-touch-icon.png">
    <link rel="apple-touch-icon" sizes="72x72" href="~/themes/orchardharvest.theme2017/images/icons/apple-touch-icon-72x72.png">
    <link rel="apple-touch-icon" sizes="114x114" href="~/themes/orchardharvest.theme2017/images/icons/apple-touch-icon-114x114.png">
}
@{
    Script.Include("http://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js").AtFoot();
    Script.Include("nav/tinynav.js").AtFoot();
    Script.Include("nav/superfish.js").AtFoot();
    Script.Include("nav/hoverIntent.js").AtFoot();
    Script.Include("nav/jquery.sticky.js").AtFoot();
    Script.Include("totop/jquery.ui.totop.js").AtFoot();
    Script.Include("slide/camera.js").AtFoot();
    Script.Include("slide/jquery.easing.1.3.min.js").AtFoot();
    Script.Include("flexslider/jquery.flexslider.js").AtFoot();
    Script.Include("fancybox/jquery.fancybox.js").AtFoot();
    Script.Include("carousel/carousel.js").AtFoot();
    Script.Include("twitter/jquery.tweet.js").AtFoot();
    Script.Include("flickr/jflickrfeed.min.js").AtFoot();
    //Script.Include("scrollbar/jquery.mCustomScrollbar.concat.min.js").AtFoot();
    //Script.Include("scrollbar/jquery.nicescroll.js").AtFoot();
    Script.Include("maps/gmap3.js").AtFoot();
    Script.Include("filters/jquery.isotope.js").AtFoot();
    Script.Include("theme-options/theme-options.js").AtFoot();
    Script.Include("theme-options/jquery.cookies.js").AtFoot();
    Script.Include("bootstrap/bootstrap.js").AtFoot();
    Script.Include("main.js").AtFoot();
}
